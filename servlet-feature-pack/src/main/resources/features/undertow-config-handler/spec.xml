<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="undertow-config-handler" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="add">
    <elem name="addr-params" value="host,profile,subsystem,configuration"/>
  </annotation>
  <refs>
    <ref feature="undertow"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" feature-id="true"/>
    <param name="configuration" default="handler" feature-id="true"/>
  </params>
</feature-spec>
