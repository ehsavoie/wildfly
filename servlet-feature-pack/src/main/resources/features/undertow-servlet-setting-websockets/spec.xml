<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="undertow-servlet-setting-websockets" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="add">
    <elem name="addr-params" value="host,profile,subsystem,servlet-container,setting"/>
  </annotation>
  <refs>
    <ref feature="undertow-servlet-container"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" feature-id="true"/>
    <param name="servlet-container" default="default" feature-id="true"/>
    <param name="setting" default="websockets" feature-id="true"/>
  </params>
</feature-spec>
