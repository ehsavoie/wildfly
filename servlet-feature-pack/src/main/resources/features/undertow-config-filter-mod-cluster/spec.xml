<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="undertow-config-filter-mod-cluster" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="add">
    <elem name="addr-params" value="host,profile,subsystem,configuration,mod-cluster"/>
  </annotation>
  <refs>
    <ref feature="undertow-config-filter"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" feature-id="true"/>
    <param name="configuration" feature-id="true"/>
    <param name="mod-cluster" feature-id="true"/>
    <param name="management-socket-binding"/>
    <param name="advertise-socket-binding"/>
    <param name="enable-http2" default="true"/>
    <param name="max-retries" default="3"/>
  </params>
</feature-spec>
