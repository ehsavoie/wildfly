<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="undertow-servlet-container" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="add">
    <elem name="addr-params" value="host,profile,subsystem,servlet-container"/>
  </annotation>
  <provides>
    <capability name="$profile.org.wildfly.undertow.servlet-container.$servlet-container"/>
  </provides>
  <refs>
    <ref feature="undertow"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" feature-id="true"/>
    <param name="servlet-container" default="default" feature-id="true"/>
  </params>
</feature-spec>
