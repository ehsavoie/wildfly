/subsystem=ejb3/strict-max-bean-instance-pool=mdb-strict-max-pool:add(derive-size="from-cpu-count",timeout="5",timeout-unit="MINUTES")
/subsystem=ejb3/strict-max-bean-instance-pool=slsb-strict-max-pool:add(derive-size="from-worker-pools",timeout="5",timeout-unit="MINUTES")
/subsystem=ejb3/thread-pool=default:add(max-threads="10",keepalive-time={"time" => 100,"unit" => "MILLISECONDS"})

/subsystem=security/security-domain=jboss-ejb-policy:add(cache-type="default")
/subsystem=security/security-domain=jboss-ejb-policy/authorization=classic:add
