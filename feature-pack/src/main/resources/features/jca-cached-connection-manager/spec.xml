<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="jca-cached-connection-manager" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="add">
    <elem name="addr-params" value="host,profile,subsystem,cached-connection-manager"/>
  </annotation>
  <refs>
    <ref feature="jca"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" default="jca" feature-id="true"/>
    <param name="cached-connection-manager" default="cached-connection-manager" feature-id="true"/>
    <param name="install" default="true"/>
  </params>
</feature-spec>
