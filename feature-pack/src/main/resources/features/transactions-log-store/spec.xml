<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="transactions-log-store" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="add">
    <elem name="addr-params" value="host,profile,subsystem,log-store"/>
  </annotation>
  <refs>
    <ref feature="transactions"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" default="transactions" feature-id="true"/>
    <param name="log-store" default="log-store" feature-id="true"/>
  </params>
</feature-spec>
