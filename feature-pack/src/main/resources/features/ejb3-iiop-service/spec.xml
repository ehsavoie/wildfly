<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="ejb3-iiop-service" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="add">
    <elem name="addr-params" value="host,profile,subsystem,service"/>
    <elem name="op-params" value="enable-by-default,use-qualified-name"/>
  </annotation>
  <refs>
    <ref feature="ejb3"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" default="ejb3" feature-id="true"/>
    <param name="service" default="iiop" feature-id="true"/>
    <param name="enable-by-default" default="false"/>
    <param name="use-qualified-name" default="false"/>
  </params>
</feature-spec>
