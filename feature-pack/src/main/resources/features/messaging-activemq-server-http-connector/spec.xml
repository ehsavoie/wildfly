<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="messaging-activemq-server-http-connector" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="add">
    <elem name="addr-params" value="host,profile,subsystem,server,http-connector"/>
  </annotation>
  <requires>
    <capability name="org.wildfly.network.socket-binding.$socket-binding"/>
  </requires>
  <refs>
    <ref feature="messaging-activemq-server"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" default="messaging-activemq" feature-id="true"/>
    <param name="server" default="default" feature-id="true"/>
    <param name="http-connector" default="http-connector" feature-id="true"/>
    <param name="socket-binding"/>
    <param name="endpoint"/>
    <param name="params" nillable="true"/>
    <param name="server-name" nillable="true"/>
  </params>
</feature-spec>
