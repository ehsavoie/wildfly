<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="webservices-client-config" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="add">
    <elem name="addr-params" value="host,profile,subsystem,client-config"/>
  </annotation>
  <refs>
    <ref feature="webservices"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" default="jca" feature-id="true"/>
    <param name="client-config" default="Standard-Client-Config" feature-id="true"/>
  </params>
</feature-spec>
