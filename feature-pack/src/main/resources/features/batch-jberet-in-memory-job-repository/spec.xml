<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="batch-jberet-in-memory-job-repository" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="add">
    <elem name="addr-params" value="host,profile,subsystem,in-memory-job-repository"/>
  </annotation>
  <refs>
    <ref feature="batch-jberet"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" default="batch-jberet" feature-id="true"/>
    <param name="in-memory-job-repository" default="in-memory" feature-id="true"/>
  </params>
</feature-spec>
