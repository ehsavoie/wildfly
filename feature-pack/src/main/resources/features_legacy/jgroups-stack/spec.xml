<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="jgroups-stack" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="add">
    <elem name="addr-params" value="host,profile,subsystem,stack"/>
    <elem name="op-params" value="statistics-enabled"/>
  </annotation>
  <provides>
    <capability name="$profile.org.wildfly.clustering.jgroups.channel-factory.$stack"/>
  </provides>
  <refs>
    <ref feature="jgroups"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" default="jgroups" feature-id="true"/>
    <param name="stack" feature-id="true"/>
    <param name="statistics-enabled" nillable="true"/>
  </params>
</feature-spec>
