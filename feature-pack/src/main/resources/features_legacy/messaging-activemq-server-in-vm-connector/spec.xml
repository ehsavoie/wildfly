<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="messaging-activemq-server-in-vm-connector" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="add">
    <elem name="addr-params" value="host,profile,subsystem,server,in-vm-connector"/>
  </annotation>
  <refs>
    <ref feature="messaging-activemq-server"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" default="messaging-activemq" feature-id="true"/>
    <param name="server" default="default" feature-id="true"/>
    <param name="in-vm-connector" default="in-vm" feature-id="true"/>
    <param name="server-id"/>
    <param name="params" nillable="true"/>
  </params>
</feature-spec>
