<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="messaging-activemq-server-jms-queue" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="add">
    <elem name="addr-params" value="host,profile,subsystem,server,jms-queue"/>
  </annotation>
  <refs>
    <ref feature="messaging-activemq-server"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" default="messaging-activemq" feature-id="true"/>
    <param name="server" default="default" feature-id="true"/>
    <param name="jms-queue" feature-id="true"/>
    <param name="entries"/>
  </params>
</feature-spec>
