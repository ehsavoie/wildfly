<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="jca-workmanager" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="add">
    <elem name="addr-params" value="host,profile,subsystem,workmanager"/>
  </annotation>
  <refs>
    <ref feature="jca"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" default="jca" feature-id="true"/>
    <param name="workmanager" default="default" feature-id="true"/>
    <param name="name" default="default"/>
  </params>
</feature-spec>
