<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="messaging-activemq-server" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="add">
    <elem name="addr-params" value="host,profile,subsystem,server"/>
  </annotation>
  <provides>
    <capability name="$profile.org.wildfly.messaging.activemq.server.$server"/>
  </provides>
  <requires>
    <capability name="$profile.org.wildfly.security.security-domain.$elytron-domain" optional="true"/>
  </requires>
  <refs>
    <ref feature="messaging-activemq"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" default="messaging-activemq" feature-id="true"/>
    <param name="server" default="default" feature-id="true"/>
    <param name="elytron-domain" nillable="true"/>
    <param name="cluster-password" nillable="true"/>
  </params>
</feature-spec>
