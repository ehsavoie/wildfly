<?xml version="1.0" encoding="UTF-8"?>
<feature-group-spec name="singleton" xmlns="urn:wildfly:pm-feature-group:1.0">
    <feature spec="singleton">
        <param name="default" value="default"/>
        <feature spec="singleton-policy">
            <param name="singleton-policy" value="default"/>
            <param name="cache-container" value="server"/>
            <feature spec="singleton-policy-election">
                <param name="election-policy" value="simple"/>
            </feature>
        </feature>
    </feature>
</feature-group-spec>
